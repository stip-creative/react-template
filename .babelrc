{
  "presets":
  [
    [
      "@babel/preset-env",
      {
        "targets":
        {
          "node": "current"
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    ["babel-plugin-root-import", {
      "paths": [{
        "rootPathPrefix": "~",
        "rootPathSuffix": ""
      }, {
        "rootPathPrefix": "&",
        "rootPathSuffix": "src/"
      }]
    }]
  ]
}